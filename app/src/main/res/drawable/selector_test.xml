<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="10dp"
        android:height="10dp"
        android:left="20dp"
        android:bottom="20dp"
        android:right="20dp"
        android:top="20dp">
        <shape
            android:shape="oval"
            android:innerRadius="10dp">
            <solid android:color="#22AAFF" />
            <size
                android:width="10dp"
                android:height="10dp" />
        </shape>
    </item>
    <item
        android:width="50dp"
        android:height="50dp">
        <shape android:shape="oval">
            <corners android:radius="25dp" />
            <stroke
                android:width="1dp"
                android:color="#ffddaa" />
            <size
                android:width="50dp"
                android:height="50dp" />
        </shape>
    </item>
</layer-list>
