<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MaterialProgressBar">
        <!--<attr name="spbStyle" format="reference" />-->
        <attr name="mpb_color" format="color" />
        <attr name="mpb_stroke_width" format="dimension" />
        <attr name="mpb_stroke_separator_length" format="dimension" />
        <attr name="mpb_sections_count" format="integer" />
        <attr name="mpb_speed" format="float" />
        <attr name="mpb_progressiveStart_speed" format="float" />
        <attr name="mpb_progressiveStop_speed" format="float" />
        <attr name="mpb_interpolator" format="enum">
            <enum name="mpb_interpolator_accelerate" value="0" />
            <enum name="mpb_interpolator_linear" value="1" />
            <enum name="mpb_interpolator_acceleratedecelerate" value="2" />
            <enum name="mpb_interpolator_decelerate" value="3" />
        </attr>
        <attr name="mpb_reversed" format="boolean" />
        <attr name="mpb_mirror_mode" format="boolean" />
        <attr name="mpb_colors" format="reference" />
        <attr name="mpb_progressiveStart_activated" format="boolean" />
        <attr name="mpb_background" format="reference" />
        <attr name="mpb_generate_background_with_colors" format="boolean" />
        <attr name="mpb_gradients" format="boolean" />
    </declare-styleable>
</resources>