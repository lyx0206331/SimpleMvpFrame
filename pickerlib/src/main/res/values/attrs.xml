<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="wheelview">
        <attr name="p_wv_gravity">
            <enum name="center" value="17" />
            <enum name="left" value="3" />
            <enum name="right" value="5" />
        </attr>
        <attr name="p_wv_textSelectedSize" format="dimension" />
        <attr name="p_wv_textUnselectedSize" format="dimension" />
        <attr name="p_wv_textColorOut" format="color" />
        <attr name="p_wv_textColorCenter" format="color" />
        <attr name="p_wv_textLabelSize" format="dimension" />
        <attr name="p_wv_textLabelColor" format="color" />
        <attr name="p_wv_dividerColor" format="color" />
        <attr name="p_wv_centerBackground" format="color" />
        <attr name="p_wv_lineSpacingMultiplier" format="float" />
        <attr name="p_wv_isLoop" format="boolean" />
        <attr name="p_wv_initPosition" format="integer" />
        <attr name="p_wv_visibleItemCount" format="integer" />
    </declare-styleable>
</resources>